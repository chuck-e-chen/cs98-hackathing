{
  "manifest_version": 2,
  "name": "Hackathing Extension 1",
  "description": "An extension to find the first link on a page and open it in a new tab",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.5.1.min.js", "content.js"]
  }],
  "browser_action": {
  "default_icon": "1.png"
  },
  "icons": {
    "16": "1.png",
    "32": "1.png",
    "48": "1.png",
    "18": "1.png"
  }
}